x.addEventListener("mouseout", z);
function nad() {
document.getElementById('output').innerHTML += "Nad<br>"
}
function klik() {
document.getElementById('output').innerHTML += "Klik<br>"
}
function z() {
document.getElementById('output').innerHTML += "z<br>"
}

