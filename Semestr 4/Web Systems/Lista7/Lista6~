<button type="button">Random</button>

<script>
  function f1() { 
    let wynik1 = Math.round(Math.random()*(5518+1));
    //Math.floor(Math.random() * (max - min)) + min;
    diva = document.getElementById("a");
    diva.innerHTML = wynik1;
    
    let wynik2 = Math.round(Math.random()*(5518+1));
    let mnoznik = Math.round(Math.random()*(5518+1));
    if (mnoznik % 2 == 0) wynik2 *= -1;
    divb = document.getElementById("b");
    divb.innerHTML = wynik2;
    
    
}

</script>
